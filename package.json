{
  "name": "nuxt-ws",
  "version": "0.0.2",
  "description": "A Nuxt module aimed to simplify the use of WebSockets.",
  "repository": "sandros94/nuxt-ws",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Sandro Circi",
    "email": "sandro.circi@digitoolmedia.com"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "docs:preview": "nuxt preview docs",
    "release": "pnpm lint && pnpm prepack && changelogen --release --push --publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "peerDependencies": {
    "crossws": ">=0.3",
    "undio": ">=0.2"
  },
  "dependencies": {
    "@nuxt/kit": "^3.16.0",
    "@standard-schema/spec": "^1.0.0",
    "@vueuse/core": "^13.0.0",
    "crossws": "^0.3.4",
    "defu": "^6.1.4",
    "undio": "^0.2.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.3.0",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.16.0",
    "@nuxt/test-utils": "^3.17.2",
    "changelogen": "^0.6.1",
    "eslint": "^9.22.0",
    "nuxt": "^3.16.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "resolutions": {
    "nuxt-ws": "workspace:*",
    "typescript": "5.6.3",
    "vue-tsc": "2.1.6"
  },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi",
      "workerd"
    ]
  }
}
